        //REGION MAJ DES QUERYSTRING ET FOLDERS EN LIVE PRODUITS/RECETTES
        function UpdateQueryString(key, value, url) {
            if (!url) url = window.location.href;
            var re = new RegExp("([?&])" + key + "=.*?(&|#|$)(.*)", "gi"),
                hash;

            if (re.test(url)) {
                if (typeof value !== 'undefined' && value !== null)
                    return url.replace(re, '$1' + key + "=" + value + '$2$3');
                else {
                    hash = url.split('#');
                    url = hash[0].replace(re, '$1$3').replace(/(&|\?)$/, '');
                    if (typeof hash[1] !== 'undefined' && hash[1] !== null)
                        url += '#' + hash[1];
                    return url;
                }
            }
            else {
                if (typeof value !== 'undefined' && value !== null) {
                    var separator = url.indexOf('?') !== -1 ? '&' : '?';
                    hash = url.split('#');
                    url = hash[0] + separator + key + '=' + value;
                    if (typeof hash[1] !== 'undefined' && hash[1] !== null)
                        url += '#' + hash[1];
                    return url;
                }
                else
                    return url;
            }
        }

        function UpdateQueryStringAndRefresh(key, value, url) {
            window.location.href = UpdateQueryString(key, value, url);
        }

        function UpdateFoldersFromSelect(elt, g, r, f, sf, ssf) {
            var option = $(elt).find(":selected");
            var key = option.data("key");
            var value = option.data("value");
            if (key != "" && value != "") {
                switch (key) {
                    case ("g"):
                        UpdateFolders(value)
                        break;
                    case ("r"):
                        UpdateFolders(g, value)
                        break;
                    case ("f"):
                        UpdateFolders(g, r, value)
                        break;
                    case ("sf"):
                        UpdateFolders(g, r, f, value)
                        break;
                    case ("ssf"):
                        UpdateFolders(g, r, f, sf, value)
                        break;
                }
            }
        }

        function UpdateFolders(g, r, f, sf, ssf) {
            var url = window.location.pathname;
            var pathArray = url.split('/');
            var newPathArray = new Array();

            newPathArray.push(pathArray[1]); //produits-{nomCommercial}.html

            if (g != null) {
                newPathArray.push(g);
                if (r != null) {
                    newPathArray.push(r);
                    if (f != null) {
                        newPathArray.push(f);
                        if (sf != null) {
                            newPathArray.push(sf);
                            if (ssf != null) {
                                newPathArray.push(ssf);
                            }
                        }
                    }
                }
            }

            newPathArray.push(pathArray[pathArray.length - 1]); //{page}.html


            var newPathname = "";
            for (i = 0; i < newPathArray.length; i++) {
                newPathname += "/";
                newPathname += newPathArray[i];
            }
            url = window.location.protocol + "//" + window.location.host + newPathname + window.location.search;
            console.log(url);
            window.location.href = url;
        }

        //END REGION
    </script>

    

    

    
